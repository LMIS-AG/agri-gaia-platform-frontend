{
  "name": "agri-gaia-platform",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "clean": "rimraf dist",
    "reinstall": "rimraf node_modules && npm install && ng build --aot",
    "build": "ng build && npm run build:dockerfile",
    "build:analyze": "ng build --stats-json && webpack-bundle-analyzer dist/agri-gaia-platform/stats.json",
    "build:dockerfile": "copyfiles --error -u 1 docker/* dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.8",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.2.8",
    "@angular/compiler": "~12.2.8",
    "@angular/core": "~12.2.8",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.2.8",
    "@angular/material": "^12.2.8",
    "@angular/material-luxon-adapter": "^12.2.8",
    "@angular/platform-browser": "~12.2.8",
    "@angular/platform-browser-dynamic": "~12.2.8",
    "@angular/router": "~12.2.8",
    "@ngneat/hot-toast": "^3.4.1",
    "@ngneat/overview": "^2.0.2",
    "@ngneat/transloco": "^2.23.4",
    "@ngneat/transloco-messageformat": "^1.3.1",
    "@ngneat/until-destroy": "^8.1.4",
    "messageformat": "^2.3.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.8",
    "@angular/compiler-cli": "~12.2.8",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}

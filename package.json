{
  "name": "agri-gaia-platform",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "clean": "rimraf dist",
    "reinstall": "rimraf node_modules && npm install && ng build --aot",
    "build": "ng build && npm run build:dockerfile",
    "build-prod": "ng build --prod && npm run build:dockerfile",
    "build:analyze": "ng build --stats-json && webpack-bundle-analyzer dist/agri-gaia-platform/stats.json",
    "build:dockerfile": "copyfiles --error -u 1 docker/* dist"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular/animations": "^14.0.1",
    "@angular/cdk": "^14.0.1",
    "@angular/cli": "^14.0.1",
    "@angular/common": "^14.0.1",
    "@angular/compiler": "^14.0.1",
    "@angular/core": "^14.0.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.0.1",
    "@angular/material": "^14.0.1",
    "@angular/material-luxon-adapter": "^14.0.1",
    "@angular/platform-browser": "^14.0.1",
    "@angular/platform-browser-dynamic": "^14.0.1",
    "@angular/router": "^14.0.1",
    "@ngneat/hot-toast": "^4.1.0",
    "@ngneat/overview": "^3.0.4",
    "@ngneat/transloco": "^2.23.4",
    "@ngneat/transloco-messageformat": "^1.3.1",
    "@ngneat/until-destroy": "^8.1.4",
    "keycloak-angular": "^10.0.1",
    "keycloak-js": "^18.0.1",
    "messageformat": "^2.3.0",
    "rxjs": "7.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular-eslint/builder": "14.0.0-alpha.3",
    "@angular-eslint/eslint-plugin": "14.0.0-alpha.3",
    "@angular-eslint/eslint-plugin-template": "14.0.0-alpha.3",
    "@angular-eslint/schematics": "14.0.0-alpha.3",
    "@angular-eslint/template-parser": "14.0.0-alpha.3",
    "@angular/cli": "^14.0.1",
    "@angular/compiler-cli": "^14.0.1",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.6.4",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "overrides": {
    "keycloak-angular": {
      "@angular/common": "^14.0.0",
      "@angular/router": "^14.0.0",
      "@angular/core": "^14.0.0"
    }
  }
}
